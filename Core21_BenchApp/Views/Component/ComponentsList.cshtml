
@{
    ViewData["Title"] = "ComponentsList";

}

<h2>ComponentsList</h2>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Version</th>
            <th>Description</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
             <tr class="table table-hover">
                <td>@item.Name</td>
                <td>@item.Version</td>
                <td>@item.Description</td>
            </tr>
        }

    </tbody>
</table>


